algoritmo "41"
//Desenvolva um programa que mostre na tela a seguinte contagem:
//100 95 90 85 80 ... 0 Acabou!
var
   c, i: inteiro
inicio
      c <- 1
      i <- 100
      enquanto (c < 22) faca
         escreva(i)
         c <- c + 1
         i <- i - 5
      fimenquanto
      escreva(" Acabou!")
fimalgoritmo